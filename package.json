  "scripts": {
    "analyze": "ANALYZE=true yarn run build",
    "build": "next build",
    "check-format": "prettier -l \"cypress/**/*.js\" \"db/**/*.js\" \"lib/**/*.js\" \"migrations/**/*.js\" \"pages/**/*.js\"",
    "dev": "next",
    "docs": "cd docs && mkdocs serve",
    "cypress:run": "cypress run",
    "format": "prettier --write \"cypress/**/*.js\" \"db/**/*.js\" \"lib/**/*.js\" \"migrations/**/*.js\" \"pages/**/*.js\"",
    "jest": "jest --runInBand --detectOpenHandles --forceExit",
    "lint": "eslint cypress/ db/ lib/ migrations/ pages/",
    "migrate": "migrate --store=./db/migrations-store.js --env=.env",
    "now-build": "next build",
    "predev": "yarn",
    "pretest": "yarn",
    "semantic-release": "semantic-release",
    "start": "next start",
    "test": "yarn run lint && yarn run jest && yarn run cypress run",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
