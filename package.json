  "scripts": {
    "action": "run-p java start",
    "analyze": "ANALYZE=true yarn run build",
    "build": "next build",
    "check-format": "prettier -l \"cypress/**/*.js\" \"db/**/*.js\" \"lib/**/*.js\" \"migrations/**/*.js\" \"pages/**/*.js\"",
    "dev": "next",
    "docs": "cd docs && mkdocs serve",
    "cypress:run": "cypress run",
    "format": "prettier --write \"cypress/**/*.js\" \"db/**/*.js\" \"lib/**/*.js\" \"migrations/**/*.js\" \"pages/**/*.js\"",
    "java": "java -cp *.jar com.conveyal.analysis.BackendMain",
    "jest": "jest",
    "lint": "eslint cypress/ db/ lib/ migrations/ pages/",
    "migrate": "migrate --store=./db/migrations-store.js --env=.env",
    "now-build": "next build",
    "prebuild": "yarn",
    "predev": "yarn",
    "pretest": "yarn",
    "semantic-release": "semantic-release",
    "start": "next start",
    "test": "yarn run tsc && yarn run lint && yarn run jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "tsc": "tsc"
  },
